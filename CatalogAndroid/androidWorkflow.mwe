<workflow>
  <!--<property file="workflow.properties"/>-->
  <component class="org.eclipse.xtend.typesystem.xsd.XMLReader">
    <modelSlot value="model" />
    <uri value="resources/catalog.xml" />
    <metaModel id="mm"
      class="org.eclipse.xtend.typesystem.xsd.XSDMetaModel">
      <schemaFile value="resources/catalog.xsd" />
    </metaModel>
  </component>
  <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="AndroidCatalog::tpl::strings_xml::Root FOR model" />
    <outlet path="AndroidCatalog/res/values" />
  </component>
  <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="AndroidCatalog::tpl::TemplateActivity_java::Root FOR model" />
    <outlet path="AndroidCatalog/src/com/creocode/catalog/android" />
  </component>
    <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="AndroidCatalog::tpl::AndroidManifest_xml::Root FOR model" />
    <outlet path="AndroidCatalog" />
  </component>
  <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="AndroidCatalog::tpl::build_properties::Root FOR model" />
    <outlet path="AndroidCatalog" />
  </component>

  <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="AndroidCatalog::tpl::Catalog_java::Root FOR model" />
    <outlet path="AndroidCatalog/src/com/creocode/catalog/generator/content" />
  
    <beautifier class="org.eclipse.xpand2.output.JavaBeautifier"/>
    <fileEncoding value="UTF-8"/>
  </component>
  
  <!--
  <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="descriptorTemplate::Root FOR model" />
    <outlet path="./" />
  </component>
    <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="buildTemplate::Root FOR model" />
    <outlet path="mtj-build/" />
  </component>
  <component class="org.eclipse.xpand2.Generator">
    <metaModel idRef="mm" />
    <expand value="symbolsTemplate::Root FOR model" />
    <outlet path="mtj-build/" />
  </component>
  -->
</workflow>
